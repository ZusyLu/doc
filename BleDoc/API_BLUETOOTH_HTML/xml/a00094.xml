        <definition>sl_status_t sl_bt_coex_set_parameters</definition>
        <argsstring>(uint8_t priority, uint8_t request, uint8_t pwm_period, uint8_t pwm_dutycycle)</argsstring>
        <name>sl_bt_coex_set_parameters</name>
        <param>
          <type>uint8_t</type>
