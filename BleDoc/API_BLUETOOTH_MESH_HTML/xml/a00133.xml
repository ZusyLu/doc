        <type>sl_status_t</type>
        <definition>sl_status_t sl_btmesh_sensor_setup_server_send_cadence_status</definition>
        <argsstring>(uint16_t client_address, uint16_t elem_index, uint16_t appkey_index, uint8_t flags, uint16_t property_id, size_t params_len, const uint8_t *params)</argsstring>
